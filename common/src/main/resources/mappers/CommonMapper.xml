<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="common">

  <resultMap id="UserIdMap" type="com.bsm.oa.common.model.UserId">
    <constructor>
      <arg column="user_id" javaType="String"/>
    </constructor>
  </resultMap>
  <resultMap id="UsernameMap" type="com.bsm.oa.common.model.Username">
    <constructor>
      <arg column="username" javaType="String"/>
    </constructor>
  </resultMap>
  <resultMap id="TeamIdMap" type="com.bsm.oa.common.model.TeamId">
    <constructor>
      <arg column="team_id" javaType="String"/>
    </constructor>
  </resultMap>
  <resultMap id="HttpUrlMap" type="com.bsm.oa.common.model.HttpUrl">
    <constructor>
      <arg column="image_url" javaType="String"/>
    </constructor>
  </resultMap>

</mapper>
